{
  "name": "damianulan/laravel-form-forge",
  "type": "library",
  "description": "UI Form builder laravel package. For building forms with ease.",
  "keywords": [
    "laravel",
    "form",
    "builder",
    "ui",
    "forge"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Damian UÅ‚an",
      "email": "damian.ulan@protonmail.com",
      "homepage": "https://damianulan.me",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.3",
    "ext-json": "*",
    "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
    "mews/purifier": "^3.4"
  },
  "require-dev": {
    "mockery/mockery": "^1.5",
    "laravel/pint": "^1.0",
    "phpunit/phpunit": "^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "FormForge\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FormForge\\Tests\\": "tests"
    },
    "files": [
        "src/helper.php"
    ]
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "post-update-cmd": [
      "./vendor/bin/pint"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "FormForge\\FormForgeServiceProvider"
      ],
      "aliases": {
        "FormForge": "FormForge\\Facades\\FormForge"
      }
    }
  }
}
